def add(param1,param2):
    return param1 + param2

def centuryFromYear(year):
    if year%100 == 0:
        return(year//100)
    else:
        return((year//100)+1)

def checkPalindrome(inputString):
    i = inputString
    a = len(i)
    b = 0
    count = 1
    while b <= (a/2):
        if i[b] == i[a-b-1]:
            count = 1
            b += 1
        else:
            count = 0
            break
    if count == 1:
        return True
    else:
        return False
